package com.njwb.pojo;

import java.math.BigDecimal;
import java.util.Date;

public class User {
	private int userId;
	private String userName;
	private String userSex;
	private String userPwd;
	private Date userBirthday;
	private String userPhone;
	private BigDecimal userTariffe;
	private BigDecimal userCurrency;
	private int userStatus;
	private Date createTime;
	public User() {
		super();
	}
	public User(int userId, String userName, String userSex, String userPwd, Date userBirthday, String userPhone, BigDecimal userTariffe, BigDecimal userCurrency, int userStatus, Date createTime) {
		super();
		this.userId = userId;
		this.userName = userName;
		this.userSex = userSex;
		this.userPwd = userPwd;
		this.userBirthday = userBirthday;
		this.userPhone = userPhone;
		this.userTariffe = userTariffe;
		this.userCurrency = userCurrency;
		this.userStatus = userStatus;
		this.createTime = createTime;
	}
	public int getUserId() {
		return userId;
	}
	public void setUserId(int userId) {
		this.userId = userId;
	}
	public String getUserName() {
		return userName;
	}
	public void setUserName(String userName) {
		this.userName = userName;
	}
	public String getUserSex() {
		return userSex;
	}
	public void setUserSex(String userSex) {
		this.userSex = userSex;
	}
	public String getUserPwd() {
		return userPwd;
	}
	public void setUserPwd(String userPwd) {
		this.userPwd = userPwd;
	}
	public Date getUserBirthday() {
		return userBirthday;
	}
	public void setUserBirthday(Date userBirthday) {
		this.userBirthday = userBirthday;
	}
	public String getUserPhone() {
		return userPhone;
	}
	public void setUserPhone(String userPhone) {
		this.userPhone = userPhone;
	}
	public BigDecimal getUserTariffe() {
		return userTariffe;
	}
	public void setUserTariffe(BigDecimal userTariffe) {
		this.userTariffe = userTariffe;
	}
	public BigDecimal getUserCurrency() {
		return userCurrency;
	}
	public void setUserCurrency(BigDecimal userCurrency) {
		this.userCurrency = userCurrency;
	}
	public int getUserStatus() {
		return userStatus;
	}
	public void setUserStatus(int userStatus) {
		this.userStatus = userStatus;
	}
	public Date getCreateTime() {
		return createTime;
	}
	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}
	@Override
	public String toString() {
		return "User [createTime=" + createTime + ", userBirthday=" + userBirthday + ", userCurrency=" + userCurrency + ", userId=" + userId + ", userName=" + userName + ", userPhone=" + userPhone
				+ ", userPwd=" + userPwd + ", userSex=" + userSex + ", userStatus=" + userStatus + ", userTariffe=" + userTariffe + "]";
	}
}
