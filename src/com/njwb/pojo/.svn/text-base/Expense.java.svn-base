package com.njwb.pojo;

import java.math.BigDecimal;
import java.util.Date;

public class Expense {
	private int expId;
	private int expUserId;
	private int expGameId;
	private BigDecimal expMonetary;
	private int expOperations;
	private Date createTime;
	private Date updateTime;
	private int expDownloads;
	private String expGameName;
	public Expense() {
		super();
	}
	public Expense(int expId, int expUserId, int expGameId, BigDecimal expMonetary, int expOperations, Date createTime, Date updateTime, int expDownloads, String expGameName) {
		super();
		this.expId = expId;
		this.expUserId = expUserId;
		this.expGameId = expGameId;
		this.expMonetary = expMonetary;
		this.expOperations = expOperations;
		this.createTime = createTime;
		this.updateTime = updateTime;
		this.expDownloads = expDownloads;
		this.expGameName = expGameName;
	}
	public int getExpId() {
		return expId;
	}
	public void setExpId(int expId) {
		this.expId = expId;
	}
	public int getExpUserId() {
		return expUserId;
	}
	public void setExpUserId(int expUserId) {
		this.expUserId = expUserId;
	}
	public int getExpGameId() {
		return expGameId;
	}
	public void setExpGameId(int expGameId) {
		this.expGameId = expGameId;
	}
	public BigDecimal getExpMonetary() {
		return expMonetary;
	}
	public void setExpMonetary(BigDecimal expMonetary) {
		this.expMonetary = expMonetary;
	}
	public int getExpOperations() {
		return expOperations;
	}
	public void setExpOperations(int expOperations) {
		this.expOperations = expOperations;
	}
	public Date getCreateTime() {
		return createTime;
	}
	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}
	public Date getUpdateTime() {
		return updateTime;
	}
	public void setUpdateTime(Date updateTime) {
		this.updateTime = updateTime;
	}
	public int getExpDownloads() {
		return expDownloads;
	}
	public void setExpDownloads(int expDownloads) {
		this.expDownloads = expDownloads;
	}
	public String getExpGameName() {
		return expGameName;
	}
	public void setExpGameName(String expGameName) {
		this.expGameName = expGameName;
	}
	@Override
	public String toString() {
		return "Expense [createTime=" + createTime + ", expDownloads=" + expDownloads + ", expGameId=" + expGameId + ", expGameName=" + expGameName + ", expId=" + expId + ", expMonetary="
				+ expMonetary + ", expOperations=" + expOperations + ", expUserId=" + expUserId + ", updateTime=" + updateTime + "]";
	}
}
